@model List<CodeOverFlowProject.ViewModels.CategoryViewModel>
@{
    ViewBag.Title = "Categories";
}

<div class="row">
    <div class="col-sm-12">
        <h4>CATEGORY</h4>
        <ul class="list-group list-group-flush">
            @foreach (var item in Model)
            {
                <li class="list-group-item list-group-item-action">@item.CategoryID. @item.CategoryName</li>
            }
        </ul>
    </div>
</div>
<script>
    $("a[href='/AllCategories']").attr("id","active")
</script>

